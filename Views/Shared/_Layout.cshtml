@using MVCdeneme2.ViewModels
@{
    Layout = null;
    MVCdeneme2.Models.EntityFramework.OkulDbEntities db = new MVCdeneme2.Models.EntityFramework.OkulDbEntities();

    NavbarVM model = new NavbarVM
    {
        Siniflar = db.Sinif.ToList(),
        Dersler = db.Ders.ToList(),
        username = System.Web.HttpContext.Current.User.Identity.Name.ToString()
    };
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>_Layout</title>

    <link href="~/Content/site_.css" rel="stylesheet" />
    <link href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.1.3/css/bootstrap.css" rel="stylesheet" />
    <link href="https://cdn.datatables.net/1.10.21/css/dataTables.bootstrap4.min.css" rel="stylesheet" />
</head>
<body>

    @Html.Partial("_Navbar", model)
    <div class="container">
        @RenderBody()
    </div>

    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/DataTables/jquery.dataTables.min.js"></script>
    <script src="~/Scripts/DataTables/dataTables.bootstrap.min.js"></script>
    <script src="~/Scripts/custom.js"></script>
    <script src="~/Scripts/bootbox.min.js"></script>
    @RenderSection("scripts", false)


</body>
</html>
